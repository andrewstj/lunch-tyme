{
  "name": "restaurants",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://andrewsrealm.net/lunchtyme",
  "dependencies": {
    "@types/google-map-react": "^0.23.10",
    "@types/jest": "23.3.13",
    "@types/node": "10.12.18",
    "@types/react": "16.7.20",
    "@types/react-dom": "16.0.11",
    "bootstrap": "^4.2.1",
    "google-map-react": "^1.1.2",
    "react": "^16.7.0",
    "react-bootstrap": "^1.0.0-beta.4",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3",
    "typescript": "3.2.4"
  },
  "scripts": {
    "_format": "prettier --write  \"src/**/*.{ts,tsx,css}\" || echo \"Failed to format. Continuing...\"",
    "start": "npm run _format && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "husky": "^1.3.1",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
